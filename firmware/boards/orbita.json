{
    "build": {
        "core": "stm32",
        "cpu": "cortex-m4",
        "extra_flags": [
            "-DSTM32G4xx",
            "-DSTM32G474xx",
            "-DUSE_FULL_LL_DRIVER",
            "-DLUOSHAL=STM32G4 ",
            "-DEEPROM_USE_FLASH_PAGE=2"
        ],
        "f_cpu": "170000000L",
        "mcu": "stm32g474ret6"
    },
    "connectivity": [
        "can"
    ],
    "debug": {
        "jlink_device": "STM32G474RE",
        "openocd_target": "stm32g4x",
        "svd_path": "STM32G474xx.svd"
    },
    "frameworks": [
        "stm32cube"
    ],
    "name": "Orbita",
    "upload": {
        "maximum_ram_size": 131072,
        "maximum_size": 524288,
        "protocol": "dfu",
        "protocols": [
            "stlink",
            "dfu"
        ]
    },
    "url": "https://github.com/pollen-robotics/orbita",
    "vendor": "Pollen Robotics"
}